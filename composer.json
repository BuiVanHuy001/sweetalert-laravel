{
  "name": "buivanhuy/sweetalert-laravel",
  "description": "SweetAlert2 integration for Laravel (supports Livewire, Blade, and toasts)",
  "type": "library",
  "require": {
    "php": ">=8.0",
    "illuminate/support": ">=8.0"
  },
  "autoload": {
    "psr-4": {
      "SweetAlert\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SweetAlert\\ServiceProvider"
      ],
      "aliases": {
        "Swal": "SweetAlert\\Facades\\Swal"
      }
    }
  },
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true
}
