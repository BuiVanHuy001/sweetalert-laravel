{
  "name": "buivanhuy/sweetalert-laravel",
  "description": "Custom SweetAlert2 integration for Laravel.",
  "type": "library",
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0 || ^9.0 || ^10.0"
  },
  "autoload": {
    "psr-4": {
      "SweetAlert\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SweetAlert\\ServiceProvider"
      ],
      "aliases": {
        "Swal": "SweetAlert\\Facades\\Swal"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}